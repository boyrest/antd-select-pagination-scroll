{
  "private": false,
  "name": "@dm/xman-packages",
  "version": "0.1.9",
  "description": "xman-packages",
  "author": "baron zhu",
  "license": "MIT",
  "main": "./es/index.js",
  "keywords": [
    "React",
    "Component"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/boyrest/xman-packages.git"
  },
  "files": [
    "es"
  ],
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father-build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "prepublishOnly": "npm run build"
  },
  "module": "es/index.js",
  "typings": "es/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "peerDependencies": {
    "lodash": ">=4.17.21",
    "react": ">=16.4.2",
    "react-dom": ">=16.4.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.182",
    "@umijs/fabric": "^2.8.1",
    "@umijs/test": "^3.0.5",
    "babel-plugin-import": "^1.13.3",
    "dumi": "^1.1.0",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.1",
    "react": ">=16.4.2",
    "react-dom": ">=16.4.2",
    "yorkie": "^2.0.0"
  },
  "dependencies": {
    "ahooks": "^3.7.8",
    "antd": "^4.22.6",
    "query-string": "^8.1.0",
    "react-lines-ellipsis": "^0.15.4",
    "tinymce": "^6.1.2",
    "redux": "^4.2.1",
    "redux-saga": "^1.2.3"
  }
}
